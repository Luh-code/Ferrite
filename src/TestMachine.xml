<machine>
<alias>TestMachine</alias>
<external>false</external>
<trigger>
	<alias>1</alias>
	<type>digital</type>
</trigger>
<trigger>
	<alias>2</alias>
	<type>digital</type>
</trigger>
<trigger alias="timeout1">
	<type>timer</type>
</trigger>
<trigger alias="TestTrigger" type="value" value="0"/>
<output>
	<alias>1</alias>
	<type>digital</type>
	<value>false</value>
</output>
<state>
	<alias>state0</alias>
	<origin>false</origin>
	<begin>
		<trigger query="FROM trigger GET 'alias'=='TestTrigger'">
			<value>21</value>
		</trigger>
		<output query="FROM output GET 'alias'=='1'">
			<value>true</value>
		</output>
		<trigger query="FROM trigger GET 'alias'=='timeout1'">
			<value>5000</value>
		</trigger>
	</begin>
	<transition>
		<state query="FROM state GET 'alias'=='another state'"/>
		<if>
			<equals>
				<trigger query="FROM trigger GET 'alias'=='1'"/>
				<value>0</value>
			</equals>
		</if>
		<if>
			<equals>
				<trigger query="FROM trigger GET 'alias'=='timeout1"/>
				<value>0</value>
			</equals>
		</if>
	</transition>
	<end/>
</state>
<state>
	<alias>another state</alias>
	<origin>false</origin>
	<begin>
	</begin>
	<transition>
		<state query="ROOT FROM state GET 'alias'=='end'"/>
		<if script="1===1">
		</if>
	</transition>
	<end>
		<output>
			<query>FROM output GET 'alias'=='1'</query>
			<value>0</value>
		</output>
	</end>
</state>
</machine>